      "9\n",
      "16\n",
      "25\n",
      "36\n",
      "49\n",
      "64\n",
      "81\n"
     ]
    }
   ],
   "source": [
    "A = np.arange(10)**2\n",
    "\n",
    "for element in A:\n",
    "    print(element)"
   ]
